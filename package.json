{
  "name": "cls-console-sdk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "devPrepare": "pnpm recursive install",
    "dev": "env-cmd -f ./capi-forward/.env vite --port 8080 --host",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "tsc && vite build",
    "build:backend": "cd ./capi-forward && npm run build",
    "serve": "pm2 start ./capi-forward/dist/main.js"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.3",
    "core-js": "^3.41.0",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "history": "^4.10.1",
    "js-cookie": "^2.2.1",
    "moment": "^2.29.4",
    "pm2": "^5.2.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.3",
    "react-dom": "^17.0.2",
    "react-router": "^5.3.3",
    "react-router-dom": "^5.3.3",
    "react-use": "^17.3.2",
    "regenerator-runtime": "^0.13.9",
    "rxjs": "7.5.7",
    "universal-base64url": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@types/bootstrap": "^5.2.10",
    "@types/history": "^4.7.11",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^17.0.25",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.13.0",
    "eslint-config-tencent": "^1.0.4",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "http-server": "^14.1.0",
    "only-allow": "^1.1.0",
    "prettier": "^2.6.2",
    "stylelint-config-tencent": "^1.0.0",
    "typescript": "^4.9.5",
    "vite": "^6.2.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc --fix",
      "git add"
    ],
    "src/**/*.{html,vue,css,sass,scss,less}": [
      "stylelint --config stylelint.config.js --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">14"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
