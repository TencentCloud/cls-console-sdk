{
  "name": "cls-console-sdk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "devPrepare": "pnpm recursive install",
    "dev": "vite --port 8080",
    "build": "tsc && vite build",
    "serve": "vite preview --host --port 8080"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap": "5.1.3",
    "core-js": "3.21.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-use": "^17.3.2",
    "regenerator-runtime": "^0.13.9",
    "universal-base64url": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-react": "^1.2.0",
    "eslint": "^8.11.0",
    "eslint-config-tencent": "^1.0.3",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "http-server": "^14.1.0",
    "only-allow": "^1.1.0",
    "prettier": "^2.6.1",
    "stylelint-config-tencent": "^1.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.8.6",
    "vite-tsconfig-paths": "^3.4.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc --fix",
      "git add"
    ],
    "src/**/*.{html,vue,css,sass,scss,less}": [
      "stylelint --config stylelint.config.js --fix",
      "git add"
    ]
  }
}
