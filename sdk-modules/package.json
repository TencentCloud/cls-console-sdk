{
  "name": "@tencent/cls-sdk-modules",
  "version": "0.1.18",
  "module": "./dist/bundle.esm.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types",
    "lib"
  ],
  "scripts": {
    "prebuild": "rimraf dist types",
    "check": "tsc --noEmit",
    "build": "npm run build:rollup && npm run build:types",
    "build:rollup": "rollup -c ./rollup.config.js -m && tsc",
    "build:types": "tsc --noEmit false --declaration --emitDeclarationOnly --outDir ./types",
    "publishVersion": "npm version patch && tnpm publish"
  },
  "dependencies": {
    "clipboard": "^2.0.10",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-use": "^17.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "acorn-jsx": "^5.3.2",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "typescript": "^4.6.3"
  }
}
