{
  "name": "@tencent/cls-sdk-modules",
  "version": "1.1.6-react18",
  "module": "./dist/bundle.esm.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types",
    "lib"
  ],
  "scripts": {
    "prebuild": "rimraf dist types",
    "check": "tsc --noEmit",
    "build": "npm run build:rollup && npm run build:types",
    "build:rollup": "rollup -c ./rollup.config.js -m && tsc",
    "build:types": "tsc --noEmit false --declaration --emitDeclarationOnly --outDir ./types",
    "publishVersion": "npm run build && npm version patch && npm --registry \"https://mirrors.tencent.com/npm/\" publish"
  },
  "dependencies": {
    "clipboard": "^2.0.10",
    "history": "^4.10.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0"
  },
  "peerDependencies": {
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/history": "^4.7.11",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^17.0.23",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "acorn-jsx": "^5.3.2",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "typescript": "^4.9.5"
  }
}
